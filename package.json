    "@types/qrcode": "^1.5.5",
    "@upstash/redis": "^1.35.3",
    "@vercel/blob": "^1.1.1",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "prisma": "^6.14.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
