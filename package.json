{
  "name": "stufflibrary-temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "seed:stuff": "tsx scripts/seed-stuff-types.ts",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=verbose",
    "test:auth-regression": "./scripts/test-auth-regression.sh",
    "db:generate": "prisma generate",
    "db:push": "tsx scripts/db-safety-check.ts 'db push' && prisma db push",
    "db:migrate": "tsx scripts/db-safety-check.ts 'migrate dev' && prisma migrate dev",
    "db:studio": "prisma studio",
    "db:local:start": "docker-compose up -d postgres",
    "db:local:stop": "docker-compose down",
    "db:local:reset": "tsx scripts/db-safety-check.ts 'migrate reset' && prisma migrate reset --force",
    "db:production:deploy": "DATABASE_ENV=production tsx scripts/db-safety-check.ts 'migrate deploy' --allow-production && DATABASE_ENV=production prisma migrate deploy",
    "db:production:status": "DATABASE_ENV=production prisma migrate status",
    "vercel:build": "bash scripts/vercel-build.sh",
    "vercel:migration-status": "curl \"$VERCEL_URL/api/admin/migration-status?admin_key=$ADMIN_API_KEY\"",
    "vercel:migrate": "curl -X POST \"$VERCEL_URL/api/admin/migrate\" -H \"Authorization: Bearer $ADMIN_API_KEY\" -H \"Content-Type: application/json\" -d '{}'",
    "db:production:studio": "DATABASE_ENV=production prisma studio --browser none",
    "db:safety-check": "tsx scripts/db-safety-check.ts",
    "db:backup:monitor": "tsx scripts/backup-monitor.ts monitor",
    "db:backup:test": "tsx scripts/backup-monitor.ts test-restore",
    "db:migrate:production": "tsx scripts/migrate-production.ts",
    "supabase:branches": "tsx scripts/supabase-branch-manager.ts",
    "supabase:setup": "tsx scripts/supabase-branch-manager.ts setup",
    "seed": "echo 'Seed script not implemented yet. See issue #19'",
    "prepare": "command -v husky >/dev/null 2>&1 && husky || echo 'Husky not available, skipping git hooks setup'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/roboto": "^5.2.6",
    "@fontsource/space-grotesk": "^5.2.8",
    "@fontsource/special-elite": "^5.2.6",
    "@fontsource/syne-mono": "^5.2.6",
    "@google/genai": "^1.16.0",
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.2",
    "@mux/mux-node": "^8.8.0",
    "@mux/mux-player-react": "^3.1.1",
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "^6.14.0",
    "@tailwindcss/postcss": "^4",
    "@types/nodemailer": "^7.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/recharts": "^2.0.1",
    "@types/uuid": "^10.0.0",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "jose": "^6.0.13",
    "lru-cache": "^10.4.3",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "prisma": "^6.14.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "recharts": "^3.1.2",
    "resend": "^6.0.1",
    "sharp": "^0.34.3",
    "tailwindcss": "^4",
    "twilio": "^5.8.0",
    "typescript": "^5",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/expect": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
